{
    "name": "mybit/laravel-distancematrix-ai",
    "description": "Laravel wrapper for the DISTANCEMATRIX.AI API",
    "keywords": [
        "MyBit",
        "laravel",
        "laravel-distancematrix-ai"
    ],
    "homepage": "https://github.com/mybit/laravel-distancematrix-ai",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Heller",
            "email": "fh@my-b.it"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Mybit\\DistanceMatrix\\": "src",
            "Mybit\\DistanceMatrix\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mybit\\DistanceMatrix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mybit\\DistanceMatrix\\DistanceMatrixServiceProvider"
            ],
            "aliases": {
                "DistanceMatrix": "Mybit\\DistanceMatrix\\DistanceMatrixFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
